{% autoescape %}
<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge"/>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,400i,500" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('dist/css/main.css') }}"/>
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.png') }}"/>
    <link rel="apple-touch-icon" href="{{ asset('favicon.png') }}"/>
    {% block stylesheets %}{% endblock %}

    <title>Abipon</title>
</head>
<body>
    <section id="main-page">
        {% include 'header/navbar.html.twig' %}
        <section class="sidebar">
            {% include 'sidebar/sidebar.html.twig' %}
        </section>
        <section class="body">
            <div class="container-fluid">
                {% block body %}{% endblock %}
            </div>
        </section>
    </section>

    <script src="{{ asset('dist/js/lodash.js') }}"></script>
    <script src="{{ asset('dist/js/jquery.js') }}"></script>
    <script src="{{ asset('dist/js/tether.js') }}"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="{{ asset('dist/js/bootstrap.js') }}"></script>
    <script src="{{ asset('dist/js/index.js') }}"></script>
    {% block javascripts %}{% endblock %}

</body>
</html>
{% endautoescape %}