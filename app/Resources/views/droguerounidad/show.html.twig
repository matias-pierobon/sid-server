{% extends 'base.html.twig' %}

{% block body %}
    <h1>Droguerounidad</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ drogueroUnidad.id }}</td>
            </tr>
            <tr>
                <th>Desde</th>
                <td>{% if drogueroUnidad.desde %}{{ drogueroUnidad.desde|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Hasta</th>
                <td>{% if drogueroUnidad.hasta %}{{ drogueroUnidad.hasta|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('droguerounidad_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('droguerounidad_edit', { 'id': drogueroUnidad.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
