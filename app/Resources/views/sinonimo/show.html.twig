{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sinonimo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sinonimo.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ sinonimo.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('sinonimo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('sinonimo_edit', { 'id': sinonimo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
